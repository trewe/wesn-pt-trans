msgstr "Fan'to'mas"

#. [side]: type=Nightgaunt, id=Ghost of Navigator
#: The_Three_Elves/scenarios/5_Party_for_vultures.cfg:227
msgid "Ghost of Navigator"
msgstr "Geist des Navigators"

#. [objective]: condition=win
#: The_Three_Elves/scenarios/5_Party_for_vultures.cfg:262
msgid "Defend the city"
msgstr "Verteidigt die Stadt"

#. [event]: id=Tarsos, id=Lucia, id=Marilio, id=Tadien, id=Debra
#: The_Three_Elves/scenarios/5_Party_for_vultures.cfg:280
msgid "Tarsos"
msgstr "Tarsos"

#. [event]: id=Tarsos, id=Lucia, id=Marilio, id=Tadien, id=Debra
#: The_Three_Elves/scenarios/5_Party_for_vultures.cfg:287
msgid "Lucia"
msgstr "Lucia"

#. [event]: id=Tarsos, id=Lucia, id=Marilio, id=Tadien, id=Debra
#: The_Three_Elves/scenarios/5_Party_for_vultures.cfg:293
msgid "Marilio"
msgstr "Marilio"

#. [event]: id=Tarsos, id=Lucia, id=Marilio, id=Tadien, id=Debra
#: The_Three_Elves/scenarios/5_Party_for_vultures.cfg:299
msgid "Tadien"
msgstr "Tadien"

