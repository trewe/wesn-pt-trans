#: The_Three_Elves/scenarios/5_Party_for_vultures.cfg:144
msgid "female^Narven"
msgstr "Narven"

#. [side]: type=Goblin Pillager, id=Tarku
#: The_Three_Elves/scenarios/5_Party_for_vultures.cfg:187
msgid "Tarku"
msgstr "Tarku"

#. [side]: type=Lich, id=Fan'to'mas
