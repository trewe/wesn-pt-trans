#: data/campaigns/Sceptre_of_Fire/utils/utils.cfg:22
msgid "Gahahh..."
msgstr "Gahahh..."

#. [message]: speaker=Haldric II
#: data/campaigns/Sceptre_of_Fire/utils/utils.cfg:35
msgid "Maghah..."
msgstr "Maghah..."

#. [message]: speaker=Alanin
#: data/campaigns/Sceptre_of_Fire/utils/utils.cfg:48
msgid "Ahhgg..."
msgstr "Ahhgg..."

